{
    "name": "doefom/statamic-browser-detect",
    "description": "A Statamic tag as a wrapper for the composer package `hisorange/browser-detect` (https://github.com/hisorange/browser-detect).",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Doefom\\StatamicBrowserDetect\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Browser Detect",
            "description": "Statamic Browser Detect addon"
        },
        "laravel": {
            "providers": [
                "Doefom\\StatamicBrowserDetect\\ServiceProvider"
            ]
        }
    },
    "require": {
        "hisorange/browser-detect": "^5.0"
    }
}
